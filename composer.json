{
    "name": "simtabi/json-objects",
    "type": "library",
    "description": "Extract objects from large JSON files, endpoints or streams while saving memory.",
    "keywords": [
        "cerbero",
        "json-objects",
        "json",
        "stream",
        "parser"
    ],
    "homepage": "https://github.com/simtabi/json-objects",
    "license": "MIT",
    "authors": [{
        "name": "Simtabi",
        "email": "contact@simtabi.com",
        "homepage": "https://github.com/cerbero90",
        "role": "Developer"
    }],
    "require": {
        "php": ">=8.2",
        "psr/http-message": "*",
        "salsify/json-streaming-parser": "*"
    },
    "require-dev": {
        "mockery/mockery": ">=1.3",
        "phpunit/phpunit": ">=7.0",
        "squizlabs/php_codesniffer": ">=3.0"
    },
    "autoload": {
        "psr-4": {
            "Cerbero\\JsonObjects\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cerbero\\JsonObjects\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs --standard=PSR12 src tests",
        "fix-style": "phpcbf --standard=PSR12 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}